{
	"name": "flow_literaturereview",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "literaturereview",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "filter1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {No.} as string,",
				"          {Title Author} as string,",
				"          {key points} as string,",
				"          feature1 as string,",
				"          feature2 as string,",
				"          {target label} as string,",
				"          methods as string,",
				"          experiments as string,",
				"          accuracy as string,",
				"          {other notes} as string,",
				"          limitations as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 filter(ERROR_FUNCTION('{key points}contains(\"facial\")')) ~> filter1",
				"filter1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}